{
    "name": "cleaniquecoders/clock-io",
    "description": "Capture time in and out for anything - attendance, asset check in and out, event and many more.",
    "type": "library",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\ClockIO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\ClockIO\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
